/* page catalogue public */


#catalog_button{
width:538px;
text-align:right;
margin-bottom:10px;
}

#catalog_explain{
width:538px;
margin-bottom:25px;
font-family:Verdana, Arial, Helvetica, sans-serif;
font-size:12px;
color:#000000;
text-align:left;
}

#catalog_listing_top{
width:538px;
background-image:url(/images/www3/canvas/catalog/top_catalog.gif);
height:17px
}

#catalog_listing_container{
width:538px;
background-image:url(/images/www3/canvas/catalog/bckg_catalog.gif);
background-repeat:repeat-y;
