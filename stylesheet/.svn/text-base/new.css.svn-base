color:#000000;
text-align:left;
	
}

.image_try {
	padding-right: 20px;
}


/* asterisk emergency  layout - begin */
#asterisk{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	color: #3F3F3F;
	padding: 20px;
	background-color : #F2F2F2;
}

#asterisk a{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 14px;
	color: #9B0305;

}
/* asterisk emergency layout - end */

/* products-info reviews - begin */
.top_reviews {
	border-bottom-width: 2px;
	border-bottom-style: solid;
	border-bottom-color: #DCD9D9;
	height:15px;
}

.bottom_reviews {
	border-top-width: 2px;
	border-top-style: solid;
	border-top-color: #DCD9D9;
	height:10px;
}

.middle_reviews {
	border-top-width: 1px;
	border-top-style: solid;
	border-top-color: #AD3D3C;
	height:15px;
}

.title_reviews {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#3B3232;
	font-size:12px;
	font-weight:bold;
}

.text_reviews {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#3B3232;
	font-size:11px;
	margin-top:2px;
	margin-bottom:2px;
	padding:5px;
}
.text_reviews_last {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#3B3232;
	font-size:11px;
	margin-top:2px;
	margin-bottom:2px;
	padding:5px;
	background-color:#B2A8A8;
}
.pertinant_reviews {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#A9A0A0;
	font-size:11px;
	margin-top:1px;
	margin-bottom:5px;
	padding:5px;
}

.pertinant_reviews a {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#A9A0A0;
	font-size:11px;
	text-decoration:underline;
}

.pertinant_reviews_last {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#FFFFFF;
	font-size:11px;
	height:25px;
	vertical-align:middle;
	margin-bottom:5px;
	padding:5px;
	background-color:#B2A8A8;
}

.pertinant_reviews_last  a {
	text-align:left;
	font-family: Arial, Helvetica, sans-serif;
	color:#FFFFFF;
	font-size:11px;
	text-decoration:underline;
}

.rated_reviews {
	text-align:right;
	font-family: Arial, Helvetica, sans-serif;
	color:#A9A0A0;
	font-size:11px;
	padding:5px;
}
.rated_reviews_last{
	text-align:right;
	font-family: Arial, Helvetica, sans-serif;
	color:#FFFFFF;
	font-size:11px;
	padding:5px;
	background-color:#B2A8A8;
}
/* products-info reviews-end*/